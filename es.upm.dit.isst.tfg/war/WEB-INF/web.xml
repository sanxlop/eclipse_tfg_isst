<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Es_upm_dit_isst_tfg</servlet-name>
		<servlet-class>es.upm.dit.isst.tfg.Es_upm_dit_isst_tfgServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Es_upm_dit_isst_tfg</servlet-name>
		<url-pattern>/es_upm_dit_isst_tfg</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>es.upm.dit.isst.tfg.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>CrearTFGServlet</servlet-name>
		<servlet-class>es.upm.dit.isst.tfg.CrearTFGServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CrearTFGServlet</servlet-name>
		<url-pattern>/nuevoTFG</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>UploadTFGServlet</servlet-name>
		<servlet-class>es.upm.dit.isst.tfg.UploadTFGServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UploadTFGServlet</servlet-name>
		<url-pattern>/upload</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>SuccessUploadFileServlet</servlet-name>
		<servlet-class>es.upm.dit.isst.tfg.SuccessUploadFileServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SuccessUploadFileServlet</servlet-name>
		<url-pattern>/successupload</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ServeFileServlet</servlet-name>
		<servlet-class>es.upm.dit.isst.tfg.ServeFileServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ServeFileServlet</servlet-name>
		<url-pattern>/serve</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>principal.html</welcome-file>
	</welcome-file-list>
</web-app>
